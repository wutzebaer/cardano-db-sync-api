<config xmlns='http://www.ehcache.org/v3' xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jsr107="http://www.ehcache.org/v3/jsr107"
	xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd
							http://www.ehcache.org/v3/jsr107 http://www.ehcache.org/schema/ehcache-107-ext-3.0.xsd">
	<cache alias="buckets">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getCached">
		<expiry>
			<ttl unit="seconds">2</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getStakeInfo">
		<expiry>
			<ttl unit="hours">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getUtxos">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getReturnAddress">
		<expiry>
			<ttl unit="days">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getStakeAddress">
		<expiry>
			<ttl unit="days">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getStakeAddressByHash">
		<expiry>
			<ttl unit="days">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getStakeHashByAddress">
		<expiry>
			<ttl unit="days">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getTokenList">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getAddressTokenList">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getTokenDetails">
		<expiry>
			<ttl unit="hours">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getPoolList">
		<expiry>
			<ttl unit="hours">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getEpochStake">
		<expiry>
			<ttl unit="days">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getStatement">
		<expiry>
			<ttl unit="minutes">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getHandles">
		<expiry>
			<ttl unit="minutes">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getAddressByHandle">
		<expiry>
			<ttl unit="minutes">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getLastMint">
		<expiry>
			<ttl unit="minutes">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getOwners">
		<expiry>
			<ttl unit="hours">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="isTransactionConfirmed">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getTransactionMetadata">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getTransactionOutputs">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getTip">
		<expiry>
			<ttl unit="seconds">10</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

	<cache alias="getMinswapPools">
		<expiry>
			<ttl unit="minutes">1</ttl>
		</expiry>
		<heap unit="entries">1000000</heap>
		<jsr107:mbeans enable-statistics="true" />
	</cache>

</config>